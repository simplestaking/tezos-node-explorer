{
  "name": "tezedge-explorer",
  "version": "0.7.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mock": "npm run mock:http & npm run mock:debugger & npm run start",
    "start:sandbox": "ng serve --prod --port 80 --host sandbox.dev.tezedge.com",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build",
    "build:prod": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --prod",
    "build:docker": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=docker",
    "build:stats": "ng build --prod --stats-json",
    "analyze": "webpack-bundle-analyzer dist/tezedge-explorer/stats-es2015.json",
    "mock:http": "json-server --watch mock/http/db.json --port 3001 --routes mock/http/routes.json",
    "mock:debugger": "json-server --watch mock/debugger/db.json --port 3002 --routes mock/debugger/routes.json",
    "test": "start-server-and-test start http://localhost:4200 test:run",
    "test:run": "$(npm bin)/cypress run --browser chrome --headless",
    "test:open": "$(npm bin)/cypress open",
    "deploy": "firebase deploy",
    "postinstall": "node ./path.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.13",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "^11.2.13",
    "@angular/compiler": "^11.2.13",
    "@angular/core": "^11.2.13",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.13",
    "@angular/material": "^11.2.12",
    "@angular/platform-browser": "^11.2.13",
    "@angular/platform-browser-dynamic": "^11.2.13",
    "@angular/router": "^11.2.13",
    "@angular/service-worker": "^11.2.13",
    "@ngneat/until-destroy": "^8.0.3",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/router-store": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "@observablehq/runtime": "^4.8.2",
    "@swimlane/ngx-charts": "^16.0.0",
    "angular-notifier": "^9.0.1",
    "blake2b": "^2.1.3",
    "bs58check": "^2.1.2",
    "crypto": "^1.0.1",
    "d3": "^6.7.0",
    "d3-shape": "^2.1.0",
    "dagre-d3": "^0.6.4",
    "lodash": "^4.17.20",
    "moment-mini-ts": "^2.20.1",
    "ng-openapi-gen": "^0.17.1",
    "ngx-json-viewer": "^2.4.0",
    "process": "^0.11.10",
    "rxjs": "~6.5.5",
    "stream": "0.0.2",
    "swagger-ui-dist": "^3.51.0",
    "tezos-wallet": "^1.3.8",
    "treemap-chart": "^1.4.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.15",
    "@angular/cli": "^11.2.12",
    "@angular/compiler-cli": "^11.2.13",
    "@angular/language-service": "^11.2.13",
    "@types/d3": "^6.3.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cypress": "^7.4.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "start-server-and-test": "^1.12.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
}
